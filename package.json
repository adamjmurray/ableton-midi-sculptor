{
  "name": "ableton-midi-sculptor",
  "version": "1.0.0",
  "description": "The **Ableton MIDI Clip Variator**\r is a [Max for Live device](http://ableton.com/maxforlive)\r for [Ableton Live](http://ableton.com/live/)\r that makes it easy to create variations of MIDI clips with tools to modify velocity and timing.",
  "main": "midi-clip-variator.js",
  "dependencies": {
    "rollup": "^0.50.0",
    "rollup-plugin-cleanup": "^2.0.0",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  },
  "scripts": {
    "compile": "node_modules/.bin/tsc && cp src/main.js build/main.js",
    "bundle": "node_modules/.bin/rollup --silent -c && sed -i \"s/export {.*};//\" distrib/sculptor.js",
    "build": "npm run compile && npm run bundle",
    "deploy": "npm run build && cp distrib/sculptor.js ../ableton-midi-clip-variator/midi-clip-variator.js",
    "watch": "node_modules/.bin/nodemon -x \"npm run deploy\" -w src -e js,ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamjmurray/ableton-midi-sculptor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamjmurray/ableton-midi-sculptor/issues"
  },
  "homepage": "https://github.com/adamjmurray/ableton-midi-sculptor#readme"
}
