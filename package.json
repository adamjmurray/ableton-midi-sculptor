{
  "name": "ableton-midi-sculptor",
  "version": "1.0.0",
  "description": "The **Ableton MIDI Clip Variator**\r is a [Max for Live device](http://ableton.com/maxforlive)\r for [Ableton Live](http://ableton.com/live/)\r that makes it easy to create variations of MIDI clips with tools to modify velocity and timing.",
  "main": "midi-clip-variator.js",
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "mocha": "^5.2.0",
    "rollup": "^0.65.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "typescript": "^3.0.3"
  },
  "scripts": {
    "clean": "rm -rf build && rm -f device/sculptor.js",
    "compile": "tsc && cp src/main.js build/main.js && cp -r src/tests/. build/tests",
    "bundle": "rollup --silent -c && sed -i \"s/export {.*};//\" device/sculptor.js",
    "build": "npm run compile && npm run bundle",
    "build:watch": "node watch.js",
    "build:tests": "npm run compile && rollup --silent -c rollup.tests.js",
    "test": "npm run build:tests && mocha -c build/tests/all-tests.js",
    "test:watch": "node watch-tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamjmurray/ableton-midi-sculptor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamjmurray/ableton-midi-sculptor/issues"
  },
  "homepage": "https://github.com/adamjmurray/ableton-midi-sculptor#readme"
}
